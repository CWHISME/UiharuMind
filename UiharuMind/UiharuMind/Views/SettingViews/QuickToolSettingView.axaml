<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:settingViews="clr-namespace:UiharuMind.Views.SettingViews"
             xmlns:settingViewData="clr-namespace:UiharuMind.ViewModels.SettingViewData"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="UiharuMind.Views.SettingViews.QuickToolSettingView">

    <Design.DataContext>
        <settingViewData:QuickToolSettingViewModel />
    </Design.DataContext>

    <StackPanel>
        <settingViews:SettingPanelView SettingConfig="{Binding SettingConfig}" />
    </StackPanel>

</UserControl>