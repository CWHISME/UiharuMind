<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:settingViewData="clr-namespace:UiharuMind.ViewModels.SettingViewData"
             xmlns:settingViews="clr-namespace:UiharuMind.Views.SettingViews"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="UiharuMind.Views.SettingViews.LLamaCppSettingView">


    <Design.DataContext>
        <settingViewData:LLamaCppSettingModel />
    </Design.DataContext>

    <ScrollViewer Name="SettingScrollViewer">

        <StackPanel>
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.DebugConfig}" />
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.GeneralConfig}" />
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.CpuConfig}" />
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.ParamsConfig}" />
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.SamplingStrategiesConfig}" />
            <settingViews:SettingPanelView SettingConfig="{Binding LLamaCppSettingConfig.SpecialConfig}" />
        </StackPanel>

    </ScrollViewer>
</UserControl>