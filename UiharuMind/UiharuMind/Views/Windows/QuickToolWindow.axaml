<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="UiharuMind.Views.Windows.QuickToolWindow"
        Height="30"
        Width="300"
        Title="QuickToolWindow">

        <StackPanel Orientation="Horizontal">
                <Button Click="OnOcrBtnClock">
                        <PathIcon Data="{StaticResource TextFieldRegular}" Width="20" Height="20" />
                </Button>
                <Button Click="OnCopyBtnClock">
                        <PathIcon Data="{StaticResource CopyRegular}" Width="20" Height="20" />
                </Button>
        </StackPanel>
        
</Window>
