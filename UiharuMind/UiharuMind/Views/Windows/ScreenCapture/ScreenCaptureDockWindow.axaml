<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="50"
        x:Class="UiharuMind.Views.Windows.ScreenCapture.ScreenCaptureDockWindow"
        Title="ScreenCaptureDockWindow">
    <StackPanel Orientation="Horizontal">
        <Button Click="OnOcrBtnClick">
            <PathIcon Data="{StaticResource TextFieldRegular}" Background="Transparent" Width="20" Height="20"
                      Padding="5" />
        </Button>
        <Button Click="OnCopyBtnClick">
            <PathIcon Data="{StaticResource CopyRegular}" Background="Transparent" Width="20" Height="20" Padding="5" />
        </Button>
        <Button Click="OnSaveBtnClick">
            <PathIcon Data="{StaticResource SaveRegular}" Background="Transparent" Width="20" Height="20" Padding="5" />
        </Button>
    </StackPanel>

</Window>